import java.util.*;

public class AbecedarianChecker {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        // Read number of inputs
        int n = Integer.parseInt(sc.nextLine());

        // Read the words separated by comma
        String line = sc.nextLine();
        String[] words = line.split(",");

        // Prepare result list
        List<String> result = new ArrayList<>();

        // Check each word
        for (String word : words) {
            if (isAbecedarian(word)) {
                result.add("1");
            } else {
                result.add("0");
            }
        }

        // Print output as comma-separated string
        System.out.println(String.join(",", result));
    }

    // Method to check if a word is abecedarian
    private static boolean isAbecedarian(String word) {
        for (int i = 0; i < word.length() - 1; i++) {
            if (word.charAt(i) > word.charAt(i + 1)) {
                return false;
            }
        }
        return true;
    }
}
